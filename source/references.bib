@article{Kousoulas:2018fc,
author = {Kousoulas, S},
title = {{Shattering the black box: Technicities of architectural manipulation}},
journal = {International Journal of Architectural Computing},
year = {2018},
volume = {16},
number = {4},
pages = {295--305},
month = dec,
publisher = {SAGE PublicationsSage UK: London, England},
affiliation = {Delft University of Technology, Delft, Netherlands},
doi = {10.1177/1478077118801937},
language = {English},
rating = {0},
date-added = {2019-01-15T14:01:22GMT},
date-modified = {2019-05-09T10:50:56GMT},
abstract = {{\textcopyright} The Author(s) 2018. This article attempts to reverse a fallacy often met in architectural theories and practices: that of a supposed input which through processes of what one can broadly call translations generates a built output. The input{\textendash}output fallacy produces an architectural black box that treats both architectural thinking and doing as a mere process of projecting, representing and annotating {\textquoteleft}properly{\textquoteright} what will later be executed. On the contrary, a manipulative account of architecture as an active process of ecological engineering will pave the way for not only reversing the fallacy but also towards a particular understanding of architectural practices: architectural technicities and their reticular, affective potentials. Drawing on the theories of Gilbert Simondon, Andr{\'e} Leroi-Gourhan, Gilles Deleuze and Felix Guattari, I will examine how architecture can be genealogically approached as a reticular technicity which evolves by a reciprocal concretisation of its technical objects and a generalisation of its active practitioners: no longer the application of transcendental design rules, of symbolic deductions or statistical inductions but rather abductive heuristics of affective techniques; no input nor output but practices of sensorial amplification via material manipulation and vice versa.},
url = {http://journals.sagepub.com/doi/10.1177/1478077118801937},
local-url = {file://localhost/Users/caglaraydin/Documents/Library.papers3/Files/BE/BE583D9E-2574-446D-B2F6-DB1A53B11D6F.pdf},
file = {{BE583D9E-2574-446D-B2F6-DB1A53B11D6F.pdf:/Users/caglaraydin/Documents/Library.papers3/Files/BE/BE583D9E-2574-446D-B2F6-DB1A53B11D6F.pdf:application/pdf}},
