@phdthesis{Aksoy:2001vq,
author = {Aksoy, Meltem},
title = {{Varolan tasar{\i}m dilleri ve yeni tasar{\i}m dilleri ba{\u{g}}lam{\i}nda bi{\c c}im gramerleri analizi}},
year = {2001},
publisher = {{\.{I}}stanbul Teknik {\"U}niversitesi},
affiliation = {Fen Bilimleri Enstit{\"u}s{\"u}},
type = {Doktora Tezi},
isbn = {850799811},
language = {Turkish},
read = {Yes},
rating = {0},
date-added = {2018-05-22T15:32:07GMT},
date-modified = {2018-12-11T17:20:34GMT},
abstract = {Form ve kompozisyon {\c c}al{\i}{\c s}malar{\i}nda kabul g{\"o}rm{\"u}{\c s} bir paradigma olan bi{\c c}im-gramerleri mimarl{\i}kta , sanat{\i}n {\c c}e{\c s}itli dallar{\i}nda ve bilgisayar destekli tasar{\i}mda ge{\c c}mi{\c s} ve {\c c}a{\u{g}}da{\c s} {\c c}e{\c s}itli tasar{\i}m stillerini/dillerini temsil etmek ve anlamak i{\c c}in dikkate de{\u{g}}er bir ba{\c s}ar{\i} ile kullan{\i}lmaktad{\i}r. Bir s{\"u}reden beri formalizmin yeni ve orjinal tasar{\i}m dilleri yaratmak konusunda kullan{\i}mma y{\"o}nelik deneysel {\c c}al{\i}{\c s}malar da s{\"u}regelmektedir .Tezin birinci b{\"o}l{\"u}m{\"U}nde bu formalizmin k{\i}sa bir tan{\i}tlm{\i} yap{\i}lmaktad{\i}r .{\.{I}}kinci b{\"o}l{\"u}mde tezin konusunu olu{\c s}turan bi{\c c}im-gramerlerinin {\c c}{\i}k{\i}{\c s} noktas{\i} olan {\"u}retim-sistemi-formalizmleri ve bu formalizmlerin temelinde varolan genel str{\"u}kt{\"u}r, bu str{\"u}kt{\"u}r{\"u}n kavranmas{\i}n{\i}n hem varolan {\"u}retim-sistemlerini anlamak hem de yeni {\"u}retim sistemlerini geli{\c s}tirebilmek i{\c c}in faydal{\i} olaca{\u{g}}{\i} kabul{\"u}yle, k{\i}saca g{\"o}zden ge{\c c}irilmektedir. B{\"o}ylece bi{\c c}im-gramerlerinin bu formalizmler i{\c c}indeki konumu netle{\c s}tirilebilmektedir .{\"U}{\c c}{\"u}nc{\"u} b{\"o}l{\"u}mde, ilk olarak bi{\c c}im-gramerlerinin temel eleman{\i} olan {\c s}ekil ve {\c s}eklin temsilive {\c s}ekil {\"u}zerinde tan{\i}mlanabilen olas{\i}l{\i} i{\c s}lemler g{\"o}zden ge{\c c}irilmektedir. Formalizmin bile{\c s}enleri s{\i}ralanmakta ve standart bi{\c c}im-grameri-formalizminin i{\c s}leyi{\c s}i tan{\i}t{\i}lmaktad{\i}r.Yine bu b{\"o}l{\"u}m i{\c c}erisinde bi{\c c}im-gramerlerinin ama{\c c}lar{\i}n{\i}n neler oldu{\u{g}}u ve hangi alanlarda kullan{\i}ld{\i}{\u{g}}{\i} tart{\i}{\c s}{\i}lmakta, konu farkl{\i} disiplinlerden {\"o}meklerle desteklenmektedir .Standart bi{\c c}im-grameri-formalizmi 1970'lerden g{\"u}n{\"u}m{\"u}ze kadar {\c c}ok farkl{\i} alanlarda kullan{\i}lmakla birlikte, temelde kapsamaya {\c c}al{\i}{\c s}t{\i}{\u{g}}{\i} tasar{\i}m alan{\i} ile birebir {\"o}rt{\"u}{\c s}emedi{\u{g}}i g{\"o}r{\"u}lmektedir. Formalizmin tasar{\i}m{\i}n yap{\i}s{\i}na daha yak{\i}nla{\c s}t{\i}r{\i}lmas{\i} amac{\i}yla son d{\"o}nemlerde, gramerin parametrize edilmesi, rengin veya bir ba{\c s}ka niteli{\u{g}}in formalizm i{\c c}inekat{\i}lmas{\i} gibi, bir{\c c}ok {\c c}aba sarfedilmektedir. Tezin d{\"o}rd{\"u}nc{\"u} b{\"o}l{\"u}m{\"u}nde bu {\c c}abalar g{\"o}zden ge{\c c}irilmekte ve formalizme, formalizmi ger{\c c}ek tasar{\i}m s{\"u}recine ve yarat{\i}c{\i}l{\i}{\u{g}}a yak{\i}nla{\c s}t{\i}rmak ba{\u{g}}lam{\i}nda yap{\i}lan katk{\i}lar tart{\i}{\c s}{\i}lmaktad{\i}r .Be{\c s}inci b{\"o}l{\"u}mde, bi{\c c}im-gramerlerinin kullan{\i}m alanlar{\i}na ve baz{\i} k{\i}s{\i}tlamalara dayand{\i}nlan s{\i}n{\i}fland{\i}rmalar g{\"o}zden ge{\c c}irilmektedir .Kullan{\i}m alanlar{\i}na g{\"o}re yap{\i}lan s{\i}n{\i}fland{\i}rma {\c c}er{\c c}evesinde, literat{\"u}rden toplanan {\"o}rnekler Ek-A ' da belli ba{\c s}l{\i} nitelikleri ile ele al{\i}nmaktad{\i}r. Ku{\c s}kusuz bug{\"u}ne kadar, T{\"u}rkiye'de ve d{\"u}nyada, bi{\c c}im-grameri-formalizmini konu alan bir{\c c}ok yay{\i}n, tez ve ara{\c s}t{\i}rma projesi ger{\c c}ekle{\c s}tirilmi{\c s}tir. Tez kapsam{\i}nda ise t{\"u}mbu {\"o}rneklere ula{\c s}madaki zorluk ve se{\c c}im kriterlerinde s{\"o}zkonusu olabilecek belirsizlik nedeniyle bu {\"o}meklem grubu literat{\"u}rde yay{\i}nlanm{\i}{\c s} 29 {\"o}rnekle s{\i}n{\i}rland{\i}r{\i}lm{\i}{\c s}t{\i}r. Bi{\c c}im-gramerleri {\"o}meklerinin b{\"u}y{\"u}k bir {\c c}o{\u{g}}unlu{\u{g}}unu elle ger{\c c}ekle{\c s}tirilen {\"o}rneklero lu{\c s}turmakla birlikte formalizmin daha efektif kullan{\i}lmas{\i}n{\i} sa{\u{g}}layaca{\u{g}}{\i} savunulan, ve bi{\c c}im-gramerinin bir bilgisayar program{\i} i{\c c}inde kodlanmas{\i} {\c s}eklinde k{\i}saca tan{\i}mlanabilecek, bi{\c c}im-grameri-yorumlay{\i}c{\i}lar{\i} ise alt{\i}nc{\i} b{\"o}l{\"u}m{\"u}n konusunu olu{\c s}turmaktad{\i}r .Yedinci b{\"o}l{\"u}mde bi{\c c}im-gramerlerinin temelinde varolan {\"U}fetici mekanizmalar kavram{\i} mimarideki stil ve tip kavramlar{\i} ile ili{\c s}kilendirilmektedir. Stiller, bi{\c c}im-gramerleri olarak adland{\i}r{\i}lan kurala dayal{\i} kompozisyonel sistemler anlam{\i}nda tan{\i}mlanmakta, stil ve tipe ait tarihten g{\"u}n{\"u}m{\"u}ze kadar geli{\c s}tirilmi{\c s} teoriler, {\"u}retici-mekanizmalar k{\i}saca g{\"o}zden ge{\c c}irilmektedir.Bu b{\"o}l{\"u}m bilgisayar destekli tasar{\i}mda tip kavram{\i}n{\i}n ele al{\i}nmas{\i} ile sonbulmaktad{\i}r .Sekizinci b{\"o}l{\"u}mde, tezin genelinde teorik ve pratik alanda ger{\c c}ekle{\c s}tirilen ve ilk yedib{\"o}l{\"u}mde belli s{\i}n{\i}fland{\i}rmalar {\c c}er{\c c}evesinde sunulan de{\u{g}}erlendirmeler {\i}{\c s}{\i}{\u{g}}{\i} alt{\i}nda formalizmin yeterli ve yetersiz oldu{\u{g}}u noktalar s{\i}ralanmaktad{\i}r.Bu ba{\u{g}}lamda konu yarat{\i}c{\i}l{\i}k anlam{\i}nda ele al{\i}nmakta ve formalizmin bug{\"u}nk{\"u} kullan{\i}m alanlar{\i}na ve yarat{\i}c{\i} tasar{\i}m prati{\u{g}}indeki potansiyel kullan{\i}m{\i}na y{\"o}nelik k{\i}sa bir de{\u{g}}erlendirme yap{\i}lmaktad{\i}r.Tezin son b{\"o}l{\"u}m{\"u}n{\"u} olu{\c s}turan dokuzuncu b{\"o}l{\"u}m{\"u}n temel tart{\i}{\c s}ma konusu bi{\c c}im-grameri-formalizminin yarat{\i}c{\i} olup olamayaca{\u{g}}{\i} konusundad{\i}r. Bi{\c c}im-grameri {\c c}al{\i}{\c s}ma alan{\i}nda yarat{\i}c{\i}l{\i}k farkl{\i} alanlarda s{\"o}zkonusu olmaktad{\i}r.{\c{S}}u anki uygulamalar{\i}n genelinde yarat{\i}c{\i}l{\i}k sonu{\c c} {\"u}r{\"u}nde olmaktan {\c c}ok s{\"u}recin kendisindedir .B{\"o}yle bir s{\"u}re{\c c} sonucunda ger{\c c}ek tasar{\i}m s{\"u}recinde oldu{\u{g}}u gibi {\"u}r{\"u}n{\"u}n kendisi de yarat{\i}c{\i} olabilir mi? Bi{\c c}im-grameri-formalizmi bug{\"u}n geldi{\u{g}}i noktada i{\c s}in mekanik y{\"o}n{\"u}yle ilgili bir{\c c}ok problemini {\c c}{\"o}zm{\"u}{\c s} g{\"o}r{\"u}nnmektedir. Bu haliyle e{\u{g}}itimde, 