journal = {IEEE Transactions on Visualization and Computer Graphics},
keywords = {Model synthesis,procedural modeling},
pages = {715--728},
pmid = {20733235},
title = {{Model synthesis: A general procedural modeling algorithm}},
volume = {17},
year = {2011}
}
