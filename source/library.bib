@inproceedings{Weissenberg2013,
abstract = {Urban models are key to navigation, architecture and entertainment. $\backslash$nApart from visualizing facades, a number of tedious tasks remain largely manual $\backslash$n(e.g. compression, generating new facade designs and structurally comparing $\backslash$nfacades for classification, retrieval and clustering). We propose a novel $\backslash$nprocedural modelling method to automatically learn a grammar from a set of $\backslash$nfacades, generate new facade instances and compare facades. To deal with the $\backslash$ndifficulty of grammatical inference, we reformulate the problem. Instead of $\backslash$ninferring a compromising, one-size-fits-all, single grammar for all tasks, we $\backslash$ninfer a model whose successive refinements are production rules tailored for $\backslash$neach task. We demonstrate our automatic rule inference on datasets of two $\backslash$ndifferent architectural styles. Our method supercedes manual expert work and $\backslash$ncuts the time required to build a procedural model of a facade from several days $\backslash$nto a few milliseconds.},
author = {Weissenberg, Julien and Riemenschneider, Hayko and Prasad, Mukta and {Van Gool}, Luc},
booktitle = {Proceedings of the IEEE Computer Society Conference on Computer Vision and Pattern Recognition},
doi = {10.1109/CVPR.2013.31},
