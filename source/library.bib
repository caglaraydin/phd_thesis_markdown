pages = {67},
title = {{A Parametric Quasi-Grammar for Generating a Flexible Urban Design Tool : Shape Grammars Optimised by Evolutionary Algorithms A Parametric quasi-grammar for generating a flexible urban design tool : Shape grammars and genetic algorithms *}},
year = {2012}
